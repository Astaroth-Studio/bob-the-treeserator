    //Draw HealthBar
    draw_set_colour(c_red);
    draw_sprite_part(sHealthBarInner, 0, 0, 0, global.endXHealthBar, 48, 10, 10);
}
draw_sprite(sHealthBarFrame, 0, 10, 10);

