draw_set_font(font_mm);
draw_set_halign(fa_left);

var moneyText = "$"+string(playerMoney)+"AUS";
outlinedText(1000,1,moneyText,c_black,c_green);

//Ammo sprite and text creation linked to playerAmmo[0]
